{
  "metadata": {
    "schemaVersion": "1.0",
    "importType": "LEX",
    "importFormat": "JSON"
  },
  "resource": {
    "name": "ExampleIvrTester",
    "version": "1",
    "intents": [
      {
        "name": "PersonName",
        "version": "3",
        "fulfillmentActivity": {
          "type": "ReturnIntent"
        },
        "sampleUtterances": [
          "Steve Wozniak",
          "Alan Turing",
          "Grace Hopper"
        ],
        "slots": []
      }
    ],
    "voiceId": "Joanna",
    "childDirected": false,
    "locale": "en-US",
    "idleSessionTTLInSeconds": 300,
    "clarificationPrompt": {
      "messages": [
        {
          "contentType": "PlainText",
          "content": "Sorry, can you please repeat that?"
        }
      ],
      "maxAttempts": 5
    },
    "abortStatement": {
      "messages": [
        {
          "contentType": "PlainText",
          "content": "Sorry, I could not understand. Goodbye."
        }
      ]
    },
    "detectSentiment": false,
    "nluIntentConfidenceThreshold": 0.7,
    "enableModelImprovements": true
  }
}